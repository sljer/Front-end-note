var arr = [6,5,4,3,2,1];

    for(var j=0;j<arr.length;j++){

        for(var i=0;i<arr.length-1-j;i++){
        
            //i = arr.length-1
            // arr[i+1]  arr[arr.length]
            
            if(arr[i]>arr[i+1]){
                var temp = arr[i];
                arr[i] =arr[i+1];
                arr[i+1] = temp;
            }
        }
    }
  console.log(arr)



    
